{
  "name": "heimrichhannot/contao-twentytwenty-bundle",
  "type": "contao-bundle",
  "description": "Contao 4 bundle that offers you a before and after picture presentation with a sliding effect",
  "license": "proprietary",
  "require": {
    "php": "^7.4 || ^8.0",
    "symfony/config": "^4.4||^5.0",
    "symfony/dependency-injection": "^4.4 || ^5.0",
    "symfony/http-kernel": "^4.4 || ^5.0",
    "contao/core-bundle": "^4.13",
    "heimrichhannot/contao-utils-bundle": "^2.234.1"
  },
  "suggest": {
    "heimrichhannot/contao-encore-bundle": "Provide webpack encore asset support."
  },
  "require-dev": {
    "contao/test-case": "^4.13",
    "contao/manager-plugin": "^2.10",
    "friendsofphp/php-cs-fixer": "^v3.9.4",
    "phpunit/phpunit": "^10.2.0",
    //"php-http/guzzle6-adapter": "^1.1",
    //"php-http/message-factory": "^1.0.2",
    "php-coveralls/php-coveralls": "^2.0",
    "symfony/phpunit-bridge": "^6.0"
  },
  "config": {
    "preferred-install": "dist"
  },
  "autoload": {
    "psr-4": {
      "HeimrichHannot\\TwentyTwentyBundle\\": "src/"
    }
  },
  "extra": {
    "contao-manager-plugin": "HeimrichHannot\\TwentyTwentyBundle\\ContaoManager\\Plugin",
    "foxy": true
  },
  "conflict": {
    "heimrichhannot/contao-encore-bundle": "<1.5"
  }
}
